{
	"name": "NeuralNetwork",
    "description": "Takes an ImgFrame input, inferes and sends an NNTensor forward",
    "properties": "NeuralNetwork.properties.schema.json",
    "inputs": [
        {
            "name": "in",
            "type": "sreceiver",
            "acceptedDatatypes": [
                {
                    "datatype": "PoBuf",
                    "descendants": true
                }
            ]
        }
    ],
    "outputs": [
        {
            "name": "out",
            "type": "msender",
            "possibleDatatypes": [
                {
                    "datatype": "NNTensor"
                }
            ]
        }
    ]
}